{
    "name": "josrom/eloquent-crud-repository",
    "description": "All the basic methods to any Laravel project with Repository pattern",
    "minimum-stability": "stable",
    "keywords": ["repository-pattern", "eloquent", "orm", "laravel"],
    "license": "MIT",
    "type": "library",
    "version": "4.0.0",
    "authors": [
        {
            "name": "Jose Vicente Orts Romero",
            "email": "jvortsromero@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Eloquent\\Crud\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestModel.php",
            "tests/TestModelWithSoftDelete.php",
            "tests/TestDatabase.php"
        ]
    },
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "illuminate/database": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    }
}
